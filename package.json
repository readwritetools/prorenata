{
  "name": "prorenata",
  "description": "Script a sequence of commands that work on deep file paths",
  "version": "1.0.10",
  "author": "Joe Honton",
  "license": "CC-BY-NC-ND 4.0",
  "private": true,
  "main": "./dist/main.class.js",
  "bin": {
    "prn": "./bin/prorenata"
  },
  "scripts": {
    "build": "npm run exim && npm run app",
    "exim": "node /musings/lib/exim/src/rewrite.js ./src ./dbg main.class.js",
    "app": "node ./bin/prorenata ./test/build-appA.prn",
    "app-inspect": "node --inspect-brk=9229 ./bin/prorenata ./test/build-appA.prn",
    "minify": "node /usr/lib/node_modules/uglify-es/bin/uglifyjs ./dbg/main.class.js --output ./dist/main.class.js --compress --mangle --keep-classnames --keep-fnames --beautify quote_style=1 --ecma 8",
    "bump-version": "npm version patch && git push && git push --tags"
  },
  "dependencies": {
    "bluephrase": "git+ssh://root@nk021:/keep/bluephrase#*",
    "joezone": "git+ssh://root@nk021:/keep/joezone#*"
  },
  "files": [
    "bin",
    "dbg",
    "dist"
  ]
}
